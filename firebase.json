{
  "hosting": {
    "public": "dist",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "/en/**",
        "destination": "/en/index.html"
      },
      {
        "source": "/uk/**",
        "destination": "/uk/index.html"
      },
      {
        "source": "**",
        "destination": "/uk/index.html"
      }
    ]
  }
}
